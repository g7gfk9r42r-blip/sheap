{
  "name": "roman_app-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "smoke": "bash ../scripts/smoke.sh",
    "test:lidl-imap": "node scripts/test_lidl_imap.cjs",
    "fetch:lidl": "node tools/leaflets/fetch_lidl_leaflet.mjs",
    "extract:lidl": "node tools/leaflets/extract_lidl_offers.mjs",
    "test:crawl4ai": "node test_crawl4ai.mjs",
    "test:lidl:md": "node test_lidl_markdown.mjs",
    "test:lidl:llm": "node test_lidl_llm.mjs",
    "test:css": "node test_css_selector.mjs",
    "mock:crawl4ai": "node mock_crawl4ai.mjs",
    "test:lidl:offers": "npm run build && node test/test_lidl_offers.mjs",
    "test:lidl:offers:full": "node test/test_lidl_offers_full.mjs",
    "test:lidl:pa": "node test/test_prospektangebote_lidl.mjs",
    "test:edeka:pa": "npm run build && node test/test_edeka_prospektangebote.mjs",
    "test:edeka": "npm run build && node test/test_edeka_playwright.mjs",
    "test:edeka:pdf": "npm run build && node test/test_edeka_pdf_fetcher.mjs",
    "parse:kaufda": "npm run build && node scripts/parse_kaufda_html.mjs",
    "process:local": "npm run build && node scripts/process_local_prospekte.mjs",
    "process:all": "python3 scripts/process_all_prospekte.py",
    "fetch:edeka": "node tools/edeka/fetch_edeka_offers.mjs",
    "fetch:edeka:pdf": "npm run build && node dist/fetchers/edeka_pdf_fetcher.js",
    "test:lidl:playwright": "npm run build && node test/test_lidl_playwright.mjs",
    "view:lidl": "npm run build && node scripts/view_lidl_offers.mjs",
    "validate:lidl": "npm run build && node scripts/validate_lidl_offers.mjs",
    "find:lidl:food": "node scripts/find_lidl_food_leaflet.mjs",
    "pdf:lidl": "node tools/leaflets/fetch_lidl_leaflet.mjs",
    "process:lidl:pdf": "node tools/leaflets/process_lidl_pdf.mjs",
    "process:all:old": "npm run build && node scripts/process_all_prospekte.mjs"
  },
  "dependencies": {
    "better-sqlite3": "^9.6.0",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "fs-extra": "^11.3.2",
    "html-entities": "^2.6.0",
    "imapflow": "^1.1.1",
    "mailparser": "^3.9.0",
    "morgan": "^1.10.0",
    "openai": "^6.10.0",
    "pdf-parse": "^1.1.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.19.23",
    "pdf-lib": "^1.17.1",
    "playwright": "^1.48.0",
    "puppeteer": "^24.29.1",
    "sharp": "^0.34.5",
    "tesseract.js": "^5.1.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
